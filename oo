  <script>
    function searchLocation() {
      const location = document.getElementById('location-input').value;
      if (location) {
        document.getElementById('output').innerText = `Searching for: ${location}`;
        // Add API integration here to fetch actual place details.
      } else {
        document.getElementById('output').innerText = `Please enter a location.`;
      }
    }
